CREATE OR REPLACE PACKAGE BODY INSERTS IS

       PROCEDURE INSERTAR_CATALOGO_ALTURA(pRango) 
       AS
                 INSERT INTO ALTURA(ALTURA_ID,pRANGO)
                 VALUES(S_ALTURA_ID.NEXTVAL, pRango)
         END INSERTAR_CATALOGO_ALTURA;
         
               
       PROCEDURE INSERTAR_CATALOGO_CCOJOS(pColor)
       AS
                 INSERT INTO CCOJOS(CCOJOS_ID,pColor)
                 VALUES(S_CCOJOS_ID.NEXTVAL, pColor)
         END INSERTAR_CATALOGO_CCOJOS;
         
                 
       PROCEDURE INSERTAR_CATALOGO_CCABELLO(pColor);
       AS
                 INSERT INTO CCABELLO(CCABELLO_ID,pColor)
                 VALUES(S_CCABELLO_ID.NEXTVAL, pColor)
         END INSERTAR_CATALOGO_CCABELLO;
         
       PROCEDURE INSERTAR_CATALOGO_CONTEXTURA(pTipo); 
       AS
                 INSERT INTO CONTEXTURA(CONTEXTURA_ID,pTipo)
                 VALUES(S_CONTEXTURA_ID.NEXTVAL, pTipo)
         END INSERTAR_CATALOGO_CONTEXTURA;
               
       PROCEDURE INSERTAR_CATALOGO_CPIEL(pNombre);    
       AS
                 INSERT INTO CPIEL(CPIEL_ID,pNombre)
                 VALUES(S_CPIEL_ID.NEXTVAL, pNombre)
         END INSERTAR_CATALOGO_CPIEL;
          
       PROCEDURE INSERTAR_CATALOGO_DEPORTE(pNombre);
       AS
                 INSERT INTO DEPORTE(DEPORTE_ID,pNombre)
                 VALUES(S_DEPORTE_ID.NEXTVAL, pNombre)
         END INSERTAR_CATALOGO_DEPORTE; 
               
       PROCEDURE INSERTAR_CATALOGO_ESCOLARIDAD(pNivel);       
       --REVISAR EVENTO
       PROCEDURE INSERTAR_CATALOGO_EVENTO(pNombre,pDescripcion,pFecha,pCiudad,pPais);       
       PROCEDURE INSERTAR_CATALOGO_FRECUENCIA(pNombre);         
       PROCEDURE INSERTAR_CATALOGO_HOBBIE(pNombre);       
       PROCEDURE INSERTAR_CATALOGO_IDIOMA(pNombre);       
       PROCEDURE INSERTAR_CATALOGO_OCUPACION(pNombre);       
       PROCEDURE INSERTAR_CATALOGO_PAIS(pNombre);

END INSERTS;
