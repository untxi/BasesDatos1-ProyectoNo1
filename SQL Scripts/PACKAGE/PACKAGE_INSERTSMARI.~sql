CREATE OR REPLACE PACKAGE INSERTS IS

       PROCEDURE INSERTAR_CATALOGO_ALTURA(pRango);       
       PROCEDURE INSERTAR_CATALOGO_CCOJOS(pColor);         
       PROCEDURE INSERTAR_CATALOGO_CCABELLO(pColor);       
       PROCEDURE INSERTAR_CATALOGO_CONTEXTURA(pTipo);       
       PROCEDURE INSERTAR_CATALOGO_CPIEL(pNombre);       
       PROCEDURE INSERTAR_CATALOGO_DEPORTE(pNombre);       
       PROCEDURE INSERTAR_CATALOGO_ESCOLARIDAD(pNivel);       
       --REVISAR EVENTO
       PROCEDURE INSERTAR_CATALOGO_EVENTO(pNombre,pDescripcion,pFecha,pCiudad,pPais);       
       PROCEDURE INSERTAR_CATALOGO_FRECUENCIA(pNombre);         
       PROCEDURE INSERTAR_CATALOGO_HOBBIE(pNombre);       
       PROCEDURE INSERTAR_CATALOGO_IDIOMA(pNombre);       
       PROCEDURE INSERTAR_CATALOGO_OCUPACION(pNombre);       
       PROCEDURE INSERTAR_CATALOGO_PAIS(pNombre);

END INSERTS;
