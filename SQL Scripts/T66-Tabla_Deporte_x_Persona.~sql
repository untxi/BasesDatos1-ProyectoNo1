CREATE TABLE Deporte_x_Persona
(
       DEPORTE_x_PERSONA_ID NUMBER(6),
       CONSTRAINT PK_DEPORTE_x_PERSONA PRIMARY KEY (DEPORTE_x_PERSONA_ID),
       
       DEPORTE_ID NUMBER(6),
       CONSTRAINT FK_DEPORTE FOREIGN KEY(DEPORTE_ID),
       
       PERSONA_ID NUMBER(6),
       CONSTRAINT FK_PERSONA FOREIGN KEY (PERSONA_ID),
       
       FRECUENCIA_ID NUMBER(6),
       CONSTRAINT FK_FRECUENCIA FOREIGN KEY (FRECUENCIA_ID)
);
